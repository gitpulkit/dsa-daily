class Solution:
    def permute(self, nums: List[int]) -> List[List[int]]:
        path = []
        res = []
        def backtrack():
            if len(path) == len(nums):
                res.append(path[:])
                return
            for num in nums:
                if num in path:Â     
                    continue
                path.append(num)
                backtrack()
                path.pop()
            
        backtrack()
        return res
