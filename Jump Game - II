class Solution:
    def jump(self, nums: List[int]) -> int:

        dp = [math.inf]*len(nums)
        n = len(nums)
        dp[-1] = 0 
        for i in range(n-2, -1, -1):
            farthest = min(n-1,i+nums[i])
            for j in range(i+1, farthest+1):
                dp[i] = min(dp[i], 1 + dp[j])
        return dp[0]
