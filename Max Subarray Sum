class Solution:
    def maxSubArray(self, nums: List[int]) -> int:
        # let dp[i] be the best possible sum till that index
        n = len(nums)
        dp = [0]*n
        dp[0] = nums[0]
        max_so_far = nums[0]

        for i in range(1, len(nums)):
            dp[i] = max(nums[i], dp[i-1]+nums[i])
            max_so_far = max(max_so_far, dp[i])
        return max_so_far
